<body class='orderPage'>
    @Html.Partial("~/Views/Shared/Header.cshtml")
    
    <!-- Start Footer -->
    @*@Html.Partial("~/Views/Shared/Footer.cshtml")*@
    <!-- End Footer -->
    <!-- jQuery library -->
    <script src="~/assets/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/assets/js/bootstrap.js"></script>
    <!-- Application js -->
    
      
    <script>
        function startHotel(btnId) {
            $('#' + btnId).removeClass('btn-default').addClass('btn-success').text('Stop Hotel');
        }
        function stopHotel(btnId) {
            0
            $('#' + btnId).removeClass('btn-success').addClass('btn-default').text('Start Hotel');;
        }
        $(document).on('click', '#startHotel.btn-default', function () {
            var btnId = $(this).attr('id');
            startHotel(btnId)
        });
        $(document).on('click', '#startHotel.btn-success', function () {
            var btnId = $(this).attr('id');
            stopHotel(btnId);
        });
    </script>

</body>
